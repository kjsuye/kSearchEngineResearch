<h1>Search</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for([@student_user, @query]) do |f| %>
      <div class="input-group">
      <%= f.text_field :originalQueryString, value: session[:pastQueryString], class: 'form-control' %>
      <span class="input-group-btn">
      <%= f.submit "Go!", class: "btn btn-default" %>
      </span>
      </div>
    <% end %>
  </div>
</div>


