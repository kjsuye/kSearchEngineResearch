<% provide(:title, "Log In") %>
<h1>Log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@student_user) do |f| %>

      <%= f.label "Log In Code" %>
      <%= f.text_field :logInCode, class: 'form-control' %>

      <%= f.label "Grade" %>
      <%= f.text_field :grade, class: 'form-control' %>

      </br>
      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>

<%= @class %>
